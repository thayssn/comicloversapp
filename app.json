{
  "expo": {
    "name": "Comic Lovers",
    "slug": "comic-lovers-app",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "2.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.paperball.comiclovers"
    }
  }
}
